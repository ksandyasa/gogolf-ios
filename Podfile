source 'https://github.com/CocoaPods/Specs.git'

use_frameworks!
platform :ios, '8.0'

def shared_pods
    pod 'MidtransKit'
    pod 'MidtransCoreKit'
end

target 'GoGolf' do
    pod 'AFNetworking', '~> 3.0'
    pod 'MBProgressHUD', '~> 0.9.2'
    pod 'GoogleMaps'
    pod 'TTRangeSlider'
    pod 'StepSlider'
    pod 'Firebase/Core'
    pod 'Firebase/Messaging'
    pod 'CarbonKit', '~>2.1'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'Bolts'
    pod 'GSKStretchyHeaderView'
    pod 'MaterialControls', '~> 1.2.2'
    shared_pods
    
end
